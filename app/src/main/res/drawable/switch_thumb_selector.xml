<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/color_ffffff" />
            <size android:width="@dimen/switch_width" android:height="@dimen/switch_height" />
            <corners
                    android:bottomRightRadius="@dimen/switch_radius" android:bottomLeftRadius="@dimen/switch_radius"
                    android:topLeftRadius="@dimen/switch_radius" android:topRightRadius="@dimen/switch_radius"/>
            <stroke android:width="@dimen/switch_thumb_stroke" android:color="@color/color_cccccc" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/color_ffffff" />
            <size android:width="@dimen/switch_width" android:height="@dimen/switch_height" />
            <corners
                    android:bottomRightRadius="@dimen/switch_radius" android:bottomLeftRadius="@dimen/switch_radius"
                    android:topLeftRadius="@dimen/switch_radius" android:topRightRadius="@dimen/switch_radius"/>
            <stroke android:width="@dimen/switch_thumb_stroke" android:color="@color/color_3073FF" />
        </shape>
    </item>
</selector>